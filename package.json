{
  "name": "a4c-frontend",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:batched": "node --expose-gc scripts/run-all-tests-batched.js",
    "test:memory": "node scripts/memory-test-runner.cjs",
    "test:memory:verbose": "node scripts/memory-test-runner.cjs --verbose",
    "test:memory:snapshots": "node scripts/memory-test-runner.cjs --enable-snapshots",
    "test:memory:strict": "node scripts/memory-test-runner.cjs --heap-threshold 200 --growth-threshold 0.05",
    "test:memory:coverage": "node scripts/memory-test-runner.cjs --enable-coverage",
    "test:memory:single": "node --expose-gc scripts/run-single-test.js",
    "test:memory:report": "node scripts/generate-memory-report.js",
    "test:memory:monitor": "node --expose-gc scripts/test-memory-monitor.cjs --dashboard",
    "test:memory:dashboard": "node --expose-gc scripts/memory-dashboard.cjs",
    "test:memory:dashboard:demo": "node --expose-gc scripts/memory-dashboard.cjs --demo",
    "test:memory:dashboard:enhanced": "node --expose-gc scripts/demo-memory-dashboard.cjs",
    "test:memory:dashboard:enhanced:fast": "node --expose-gc scripts/demo-memory-dashboard.cjs --fast",
    "test:memory:profiler": "node --expose-gc ./scripts/test-memory-profiler.cjs",
    "test:memory:legacy": "node --expose-gc scripts/enhanced-test-runner.js",
    "precommit:memory": "node --expose-gc scripts/pre-commit-memory-check.cjs",
    "precommit:memory:quick": "PRE_COMMIT_QUICK_MODE=true node --expose-gc scripts/pre-commit-memory-check.cjs",
    "precommit:memory:detailed": "PRE_COMMIT_DETAILED_MODE=true node --expose-gc scripts/pre-commit-memory-check.cjs",
    "precommit:memory:strict": "PRE_COMMIT_HEAP_THRESHOLD_MB=25 PRE_COMMIT_PEAK_THRESHOLD_MB=100 node --expose-gc scripts/pre-commit-memory-check.cjs",
    "test:precommit": "node --expose-gc scripts/test-precommit-workflow.cjs",
    "coverage": "vitest run --coverage",
    "prepare": "husky",
    "memory:ci": "node --expose-gc scripts/ci-memory-detection.js --impact-analysis",
    "memory:ci:full": "node --expose-gc scripts/ci-memory-detection.js --full",
    "memory:ci:changed": "echo \"$CHANGED_FILES\" | node --expose-gc scripts/ci-memory-detection.js --changed-files",
    "memory:baseline:update": "node scripts/ci-memory-baseline.js update",
    "memory:baseline:download": "node scripts/ci-memory-baseline.js download",
    "memory:baseline:validate": "node scripts/ci-memory-baseline.js validate",
    "memory:report:pr": "node scripts/ci-memory-reporter.js",
    "memory:charts": "node scripts/generate-memory-charts.js",
    "memory:check": "node scripts/check-memory-thresholds.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lars-tice/A4C-FrontEnd.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lars-tice/A4C-FrontEnd/issues"
  },
  "homepage": "https://github.com/lars-tice/A4C-FrontEnd#readme",
  "description": "",
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-context-menu": "^2.2.15",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/postcss": "^4.1.11",
    "@vitejs/plugin-react": "^4.7.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.536.0",
    "mobx": "^6.13.7",
    "mobx-react-lite": "^4.1.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-day-picker": "^9.8.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-resizable-panels": "^3.0.4",
    "recharts": "^3.1.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2",
    "vite": "^7.0.6"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.3",
    "chalk": "^5.6.0",
    "chart.js": "^4.5.0",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "heapdump": "^0.3.15",
    "husky": "^9.1.7",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "playwright": "^1.54.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2",
    "v8-profiler-next": "^1.10.0",
    "vitest": "^3.2.4"
  }
}
