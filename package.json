{
  "name": "a4c-frontend",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prepare": "husky",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "knip": "knip",
    "knip:fix": "knip --fix",
    "knip:production": "knip --production",
    "knip:watch": "knip --watch",
    "docs:validate": "npm run scripts:build && node dist/scripts/documentation/validate-docs.js",
    "docs:check-alignment": "npm run scripts:build && node dist/scripts/documentation/check-doc-alignment.js",
    "docs:generate-metrics": "npm run scripts:build && node dist/scripts/documentation/generate-metrics-dashboard.js",
    "docs:cli": "npm run scripts:build && node dist/scripts/cli/index.js",
    "docs:check": "npm run docs:validate && npm run docs:check-alignment",
    "docs:check:quick": "npm run docs:check-alignment",
    "docs:lint": "markdownlint docs/**/*.md README.md CLAUDE.md",
    "docs:fix": "markdownlint --fix docs/**/*.md README.md CLAUDE.md",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "scripts:build": "tsc --project tsconfig.scripts.json",
    "scripts:dev": "tsc --project tsconfig.scripts.json --watch",
    "scripts:check-alignment": "node dist/scripts/documentation/check-doc-alignment.js",
    "scripts:generate-metrics": "node dist/scripts/documentation/generate-metrics-dashboard.js",
    "test:scripts": "vitest --config vitest.config.scripts.ts",
    "test:scripts:run": "vitest run --config vitest.config.scripts.ts",
    "test:scripts:coverage": "vitest run --coverage --config vitest.config.scripts.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Analytics4Change/A4C-FrontEnd.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Analytics4Change/A4C-FrontEnd/issues"
  },
  "homepage": "https://github.com/Analytics4Change/A4C-FrontEnd#readme",
  "description": "",
  "dependencies": {
    "@radix-ui/react-checkbox": "1.3.2",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-slot": "1.2.3",
    "@supabase/supabase-js": "^2.58.0",
    "@tailwindcss/postcss": "4.1.11",
    "@vitejs/plugin-react": "4.7.0",
    "@zitadel/react": "^1.1.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "fuse.js": "7.1.0",
    "lucide-react": "0.536.0",
    "mobx": "6.13.7",
    "mobx-react-lite": "4.1.0",
    "oidc-client-ts": "^3.3.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-router-dom": "7.8.2",
    "tailwind-merge": "3.3.1",
    "tailwindcss-animate": "1.0.7",
    "vite": "7.0.6"
  },
  "devDependencies": {
    "@playwright/test": "1.54.2",
    "@types/cli-progress": "3.11.5",
    "@types/node": "24.3.0",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "@typescript-eslint/eslint-plugin": "8.39.0",
    "@typescript-eslint/parser": "8.39.0",
    "@vitest/coverage-v8": "2.1.8",
    "autoprefixer": "10.4.21",
    "chalk": "4.1.2",
    "cli-progress": "3.12.0",
    "commander": "12.1.0",
    "eslint": "9.32.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "glob": "10.3.10",
    "husky": "9.1.7",
    "knip": "5.63.0",
    "markdownlint-cli": "0.37.0",
    "ora": "5.4.1",
    "playwright": "1.54.2",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.12",
    "typescript": "5.9.2",
    "vitest": "2.1.8"
  }
}
